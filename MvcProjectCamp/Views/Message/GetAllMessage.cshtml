@model List<EntityLayer.Concrete.Message>
@{
    ViewBag.Title = "GetAllMessage";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


@using (Html.BeginForm("GetAllMessage", "Message", FormMethod.Post))
{
    <p>
        Aranacak kelime @Html.TextBox("p")
        <input type="submit" value="Ara" />
    </p>
}

@foreach (var item in Model)
{
    <p>@item.ReceiverMail</p>
}
